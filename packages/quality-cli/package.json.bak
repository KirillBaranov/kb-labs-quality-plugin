{
  "name": "@kb-labs/quality-cli",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Quality Plugin CLI commands for monorepo analysis, health checks, and automated fixes.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugin-manifest": {
      "import": "./dist/manifest.js",
      "types": "./dist/manifest.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "kb": {
    "manifest": "./dist/manifest.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "pretype-check": "pnpm --filter @kb-labs/quality-cli build",
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kb-labs/sdk": "link:../../../kb-labs-sdk/packages/sdk",
    "@kb-labs/quality-contracts": "workspace:*",
    "@kb-labs/quality-core": "workspace:*"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@kb-labs/plugin-runtime": "link:../../../kb-labs-plugin/packages/plugin-runtime",
    "@types/node": "^24.3.3",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "globby": "^11.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
